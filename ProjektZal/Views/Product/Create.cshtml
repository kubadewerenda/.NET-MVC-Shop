@model ProjektZal.Models.Product

<h2>Dodaj produkt</h2>
<form asp-action="Create" method="post">
    <div>
        <label>Nazwa</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label>Cena</label>
        <input asp-for="Price" />
    </div>
    <button type="submit">Dodaj</button>
</form>
